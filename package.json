    "mathpix-markdown-it": "^2.0.3",
    "multer": "1.4.5-lts.1",
    "newrelic": "^11.10.0",
    "nodemailer": "^6.9.8",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "postgres": "^3.4.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "twilio": "^4.20.0",
    "uuid": "^9.0.1",
    "window": "^4.2.7"
