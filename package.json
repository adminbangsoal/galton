{
  "name": "newton",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:deploy": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "generate": "drizzle-kit generate:pg",
    "db:push": "npx drizzle-kit push:pg",
    "db:seed": "ts-node ./src/database/drizzle/seed.ts",
    "db:push-dev": "npx drizzle-kit push:pg --config=./drizzle-dev.config.ts",
    "db:migrate": "ts-node ./src/database/migrate.ts",
    "db:createmigrations": "drizzle-kit generate:pg",
    "db:dropmigration": "drizzle-kit drop",
    "db:studio": "drizzle-kit studio"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.454.0",
