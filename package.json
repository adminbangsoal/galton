    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "18.11.18",
    "@types/nodemailer": "^6.4.14",
    "@types/passport-jwt": "^3.0.12",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "commitizen": "^4.3.0",
    "csv-parse": "^5.5.2",
    "cz-conventional-changelog": "^3.3.0",
    "drizzle-kit": "^0.19.13",
